{
    "name": "quickstart",
    "forwardPorts": [5000],
    "build": {
        "dockerfile": "Dockerfile"
    },
    "runArgs": [
        "--platform=linux/amd64",
        "-p",
        "5000:5000"
    ],
    "customizations": {
        "vscode": {
            "settings": {},
            "extensions": [
                "ms-vscode.cpptools"
            ]
        }
    },
    "portsAttributes": {
        "5000": {
            "label": "flask_5000"
        }
    },
    "postAttachCommand": "bash .devcontainer/setup.sh"
}